  nome         String  @db.VarChar(100)
  sexo         String? @db.VarChar(15)
  cargo        String? @db.VarChar(30)
  tratamento   String? @db.VarChar(5)
  email        String? @db.VarChar(50)
  dtNascimento String? @db.VarChar(8)
  cpf          String? @db.Char(12)
